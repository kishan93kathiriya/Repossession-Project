<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="vo.LoginVo" table="master_table" >
		
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		
		<property name="un" type="java.lang.String">
			<column name="un" />
		</property>

		<property name="pw" type="java.lang.String">
			<column name="pw" />
		</property>
		
		<property name="user_type" type="java.lang.String">
			<column name="user_type" />
		</property>
		
		<property name="authent" type="java.lang.String">
			<column name="authent" />
		</property>
				
	</class>
</hibernate-mapping>
